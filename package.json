{
  "name": "mili-ui",
  "version": "1.0.0",
  "description": "",
  "files": [
    "dist"
  ],
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.umd.cjs"
    },
    "./*.css": {
      "import": "./dist/*.css",
      "require": "./dist/*.css"
    }
  },
  "scripts": {
    "format": "prettier-standard 'src/**/*.ts'",
    "precommit": "lint-staged",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "watch": "npm-watch",
    "optimize-icons": "svgo resources/icons -o resources/icons/min",
    "generate-icon-src": "npm run optimize-icons && node src/scripts/icons/generate-icon-components.mjs",
    "build": "vite build"
  },
  "watch": {
    "build": {
      "patterns": [
        "./src/*"
      ],
      "extensions": "css, scss, tsx, ts"
    }
  },
  "author": "miliracle",
  "license": "ISC",
  "devDependencies": {
    "@storybook/addon-essentials": "^7.3.2",
    "@storybook/addon-interactions": "^7.3.2",
    "@storybook/addon-links": "^7.3.2",
    "@storybook/addon-onboarding": "^1.0.8",
    "@storybook/blocks": "^7.3.2",
    "@storybook/react": "^7.3.2",
    "@storybook/react-vite": "^7.3.2",
    "@storybook/testing-library": "^0.2.0",
    "@types/react": "^18.2.21",
    "change-case": "^4.1.2",
    "cheerio": "^1.0.0-rc.12",
    "classnames": "^2.3.2",
    "eslint": "^8.47.0",
    "eslint-plugin-storybook": "^0.6.13",
    "eslint-plugin-typescript": "^0.14.0",
    "handlebars": "^4.7.8",
    "husky": "^8.0.3",
    "lint-staged": "^14.0.1",
    "npm-watch": "^0.11.0",
    "prettier-standard": "^16.4.1",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rollup-plugin-copy": "^3.5.0",
    "sass": "^1.66.1",
    "storybook": "^7.4.0",
    "svgo": "^3.0.2",
    "typescript": "^5.2.2",
    "vite": "^4.4.9",
    "vite-plugin-dts": "^3.5.3"
  },
  "peerDependencies": {
    "classnames": "^2.3.2",
    "react": "^18.2.0",
    "normalize.css": "^8.0.1"
  }
}
